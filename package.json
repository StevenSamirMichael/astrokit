{
  "name": "astrokit",
  "version": "0.1.1",
  "description": "Toolkit for solar system ephemerides and satellite propagation via SGP4",
  "keywords": [
    "solar",
    "sun",
    "satellite",
    "sgp4",
    "ephemerides",
    "JPL",
    "astronomy",
    "solar system",
    "solarsystem",
    "moon",
    "planetary ephemerides",
    "planets"
  ],
  "author": {
    "name": "Steven Michael",
    "email": "ssmichael@gmail.com",
    "url": "https://github.com/StevenSamirMichael/"
  },
  "license": "ISC",
  "homepage": "https://github.com/StevenSamirMichael/astrokit",
  "type": "module",
  "main": "dist/astrokit.js",
  "module": "src/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenSamirMichael/astrokit.git"
  },
  "scripts": {
    "test": "tape 'test/**/*-test.js' && eslint src",
    "build": "rm -rf dist && mkdir dist && rollup --banner \"$(preamble)\" -f umd -n ak -o dist/astrokit.js -- src/index.js",
    "build:min": "npm run build && uglifyjs  dist/astrokit.js -c -m -o dist/astrokit.min.js",
    "prepublish": "npm run build:min"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "package-preamble": "^0.1.0",
    "rollup": "^2.57.0",
    "tape": "^5.3.1",
    "uglify-js": "^3.14.2"
  }
}